package arrays;

public class Merge_Sorted_Arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr1 = new int[5];
		int[] arr2 = {3,9,12,14};
		int j = arr2.length;
		int k =arr1.length-1;
	
		arr1[1]=1;
		arr1[2]=5;
		arr1[4]=10;
		int i= k-j-1;
		while(j>=0 || k >=0){
			if(arr1[i]<arr2[j]){
				arr1[k]=arr2[j];
				j--;
			}else{
				arr1[k]=arr1[i];
				i--;
			}
			k--;
		}
	}

}
